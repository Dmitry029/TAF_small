<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="PositiveTestSuite" verbose="1">


	<test name="PositiveLoginTest with profile">
		<parameter name="chromeProfile" value="chromeProfile"></parameter>
		<classes>
			<class name="com.herokuapp.theinternet.tests.loginPageTests.PositiveLoginTests" />
